    "def enum(L):\n",
    "    result=[]\n",
    "    n=len(L)\n",
    "    for i in range(n-1):\n",
    "        string= f\"{str(i+1)}. {L[i]}\"\n",
    "        result.append(string)\n",
    "    return result"
   ]
